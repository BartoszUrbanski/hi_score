    "url": "https://github.com/mmikowski/hi_score/issues"
  },
  "homepage": "https://github.com/mmikowski/hi_score",
  "dependencies": {
    "coveralls": "^2.11.14",
    "istanbul": "^0.4.5",
    "jquery": "3.1.1",
    "jquery.event.dragscroll": "^0.9.3",
    "jquery.event.gevent": "^1.1.6",
    "jquery.event.ue": "^1.3.2",
    "jquery.scrolli": "^1.0.0",
    "jquery.urianchor": "^1.3.5",
    "jsdom": "^9.8.0",
    "jslint": "^0.9.6",
    "node-inspector": "^0.12.5",
    "nodeunit": "^0.9.1",
    "powercss": "^1.3.5",
    "taffydb": "^2.7.3",
    "uglifycss": "0.0.25",
    "uglifyjs": "^2.4.10"
  },
