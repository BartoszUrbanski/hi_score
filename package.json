{
  "name": "hi_score",
  "version": "0.6.14",
  "description": "Are you ready to get off the framework-of-the-month treadmill and take control of your SPA? Hi_Score is here to help with an architecture guide, starter files, and best-in-class libraries.",
  "keywords": [
    "spa",
    "library",
    "libraries",
    "single",
    "page",
    "web",
    "application",
    "end-to-end"
  ],
  "main": "hi_score",
  "scripts": {
    "install": "bin/install.sh",
    "test": "node_modules/.bin/nodeunit test/xhi_level_0.js",
    "cover": "node_modules/.bin/istanbul cover -x '**/vendor/**' node_modules/.bin/nodeunit test/xhi_level_0.js",
    "covera": "node_modules/.bin/istanbul cover -x '**/vendor/**' node_modules/.bin/nodeunit test/xhi_level_0.js; cat coverage/lcov.info | node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmikowski/hi_score.git"
  },
